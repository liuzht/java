public class Example8_1{
    public static void main(String args[]){
        String s1, s2;
        s1 = "天道酬勤";
        s2 = "天道酬勤";
        System.out.println(s1.equals(s2));    //supposed to be true
        System.out.println(s1 == s2);         //supposed to be false
        
        String s3 = "勇者无敌";
        String s4 = "勇者无敌";
        System.out.println(s3.equals(s4));    //supposed to be true
        System.out.println(s3 == s4);         //supposed to be false
    }
}
